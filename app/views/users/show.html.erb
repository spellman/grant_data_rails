<% provide :title, "Edit user" %>

<% provide :resource_form do %>
  <%= render "shared/error_messages", resource: @user %>
  <div id="user-form">
    <%= render "users/user_edit" %>
  </div>
<% end %>

<% provide :resource_display do %>
  <%= render "users/users_display" if @users %>
<% end %>

<%= render template: "layouts/form_and_display_page" %>
