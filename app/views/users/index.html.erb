<% provide :title, "Users" %>

<% provide :resource_form do %>
  <%= render "shared/error_messages", resource: @user %>
  <%= render "users/user_form", button_text: "Create user" %>
<% end %>

<% provide :resource_display do %>
  <%= render "users/users_display" %>
  <%= paginate @users %>
<% end %>

<%= render template: "layouts/resource_index" %>
